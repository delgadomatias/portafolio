{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["components/*"],
      "@ui/*": ["components/ui/*"],
      "@icons/*": ["components/icons/*"],
      "@sections/*": ["components/sections/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"],
      "@types/*": ["types/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  },
}
