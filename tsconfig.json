{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "F:/portafolio-astro",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@ui/*": ["src/components/ui/*"],
      "@icons/*": ["src/components/icons/*"],
      "@sections/*": ["src/components/sections/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["src/assets/*"],
      "@types/*": ["src/types/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  },
}
